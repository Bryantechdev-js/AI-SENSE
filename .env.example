# EnergySense AI - Environment Configuration
# Copy this file to .env and add your OpenRouter API key

# OpenRouter Configuration (for advanced AI models)
OPENROUTER_API_KEY=Example.key
SITE_URL=http://localhost:8501/
SITE_NAME=EnergySense AI

# Model Configuration
AI_MODEL=openai/gpt-4o-mini
MAX_TOKENS=2000
TEMPERATURE=0.7

# Application Settings
DEBUG=False
LOG_LEVEL=INFO